{
  "functions": {
    "api/*.php": { "runtime": "vercel-php@0.6.0" }
  },
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    },
    {
      "src": "/public/assets/(.*)",
      "dest": "/public/assets/$1"
    },
    {
      "src": "/public/midia/(.*)",
      "dest": "/public/midia/$1"
    }
  ]
}