{
    "functions": {
      "api/*.php": { "runtime": "vercel-php@0.6.0" }
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/api/index.php"
      },
      {
        "src": "/assets/(.*)",
        "dest": "/public/assets/$1"
      },
      {
        "src": "/midia/(.*)",
        "dest": "/public/midia/$1"
      }
    ]
  }